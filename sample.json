{
    "name": "IITA_Casava",
    "workDir": "work/tests/missing_soils",
    "templateDir": "data/templates",
    "threads": 50,
    "sample": 300,
    "weatherDir":"data/weathers",
    "default_setup": {
        "template": "NGSP0000.CSX",
        "sites": "xy_from_vector::data/shapes/Nigeria_shapefile.shp",
        "startYear": 1984,
        "nyers": 32,
        "initialN": 5,
        "fractionalAW": 0.25,
        "icrt": 5,
        "icres": 10,
        "icren": 0.8,
        "wsta":"lookup_wth::IITA::vector::data/shapes/Nigeria_shapefile.shp::CellID",
        "id_soil": "lookup_ghr::raster::data/rasters/iita_nigeria_ghr.tif",
        "ic_layers":"generate_ic_layers::$id_soil"
    },
    "dssat": {
        "executable": "~/dssat47/dscsm047",
	"mode":"B",
	"batchFile":"PIXEL.v47"
    },
    "analysis": {
        "default_analysis" : {
            "timeseries": {"Harvest Date": "HDAT"},
            "timestep": "Y",
            "processing": [
                {"Production (kg)": "HWAM: scale during collection by default"},
                {"Production (t)": "Production (kg):scale before output by 0.001 rounded"},
                {"Harvested Area (ha)": "SCALE: round before output"},
                {"Production (t)": "Production (t): drop0 before output"}
            ],
            "output": ["Production (t)", "Harvested Area (ha)"]
        }
    },
    "runs": [
        {
            "name": "cassava",
            "analysis_outfile": "outputs/unity/sorghum_yearly.csv"
        }
    ]
}
